#!/bin/bash
#===================================================================================
# call mountExport.sh and pipe the output to a logfile
#===================================================================================

touch                           /tmp/mountExport.log
echo " "                     >> /tmp/mountExport.log 
date                         >> /tmp/mountExport.log 
/home/ana/bin/mountExport.sh >> /tmp/mountExport.log 

exit 0
