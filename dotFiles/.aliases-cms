# keeping it short
export H=$HOME
export S=$HOME/CmsSetup
export C=$HOME/cms/cmssw
export J=$HOME/cms/jobs
export PATH="$H/bin:${PATH}"

alias cq="condor_q -global $USER"
alias cqa="condor_q -global $USER -analyze"
alias compile='cd $CMSSW_BASE/src/; scram b -j8; cd -'

alias vomit='voms-proxy-init -voms cms --valid 168:00'

# ==== Run 1 Era ====

